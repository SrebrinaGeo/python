nylon_sq_m = int(input())
paint_lit = int(input())
detergent_lit = int(input())
hours_work = int(input())


price_nylon = (nylon_sq_m + 2) * 1.5
price_paint = (paint_lit * 1.1) * 14.5
price_detergent = detergent_lit * 5

sum_materials = price_nylon + price_paint + price_detergent + 0.40

sum_workers = (sum_materials * 0.3) * hours_work

final_sum = sum_materials + sum_workers

print(final_sum)
